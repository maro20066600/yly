{
  "redirects": [
    {
      "source": "/gateway.html",
      "destination": "/index.html",
      "permanent": true
    },
    {
      "source": "/main.html",
      "destination": "/index.html",
      "permanent": true
    },
    {
      "source": "/join-now.html",
      "destination": "/index.html",
      "permanent": true
    },
    {
      "source": "/coming-soon.html",
      "destination": "/index.html",
      "permanent": true
    },
    {
      "source": "/about.html",
      "destination": "/index.html",
      "permanent": true
    },
    {
      "source": "/((?!index.html|results.html|styles.css|script.js|images/|audio/).*)",
      "destination": "/index.html",
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/((?!index.html|results.html|styles.css|script.js|images/|audio/).*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex"
        }
      ]
    }
  ]
}
